<h1 style="margin-top:25px;margin-left:25px">Comparador galáctico</h1>
<hr style="margin-left:25px" />
<div class="container-fluid" style="width:1600px;height:900px;">
    <div class="comparador hd">

        <div id="main">
            <label id="toggleTexturesWrap">
                <input type="checkbox" id="toggleTextures" checked />
                <span>Texturas</span>
            </label>
            <div id="milkyway"></div>
            <div class="page page-a">
                <select required></select>
                <div class="object-size">Diámetro</div>
                <div class="object-aka"></div>
                <div class="object"></div>
            </div>
            <div class="page page-b">
                <select required></select>
                <div class="object-size"></div>
                <div class="object-aka"></div>
                <div class="object"></div>
            </div>
        </div>

        <script src="~/Scripts/datosCuerposCelestes.js"></script>
        <script src="~/Scripts/comparadorCuerposCelestes.js"></script>
    </div>
</div>
